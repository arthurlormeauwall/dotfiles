alias egeconfig='vim $EGENCIACONFIG/egealias'
alias egeconfigdir='cd $EGENCIACONFIG'

#Cd to repos 
alias railservice='int; cd rail-service;'
alias rbs='int;cd rail-booking-service;'
alias ctps='int;cd checkout-train-plugin-service;'
alias rails='int; cd rails;'
alias pad='int; cd rail-silverrail-pad;'

#Sub config
alias egeutilconfig='vim $EGENCIACONFIG/egeutilalias'
alias egerailsconfig='vim $EGENCIACONFIG/egerailsalias'
alias egehipconfig='vim $EGENCIACONFIG/egehipalias'
if [ -r $EGENCIACONFIG/egeutilalias ]; then
    source $EGENCIACONFIG/egeutilalias     
fi
if [ -r $EGENCIACONFIG/egerailsalias ]; then
    source $EGENCIACONFIG/egerailsalias     
fi
if [ -r $EGENCIACONFIG/egehipalias ]; then
    source $EGENCIACONFIG/egehipalias     
fi
